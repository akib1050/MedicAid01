@model IEnumerable<MedicAid01.Models.Doctor>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<p>
   @Html.ActionLink("For a New Account click here!", "../Administrators1/Create")
</p>

<p>
    @Html.ActionLink("For Patients info click here", "../Patients1/Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DoctorFirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DoctorLastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Specialize)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DoctorPhoneNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DoctorFirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DoctorLastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Specialize)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DoctorPhoneNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.DoctorId }) |
                @Html.ActionLink("Details", "Details", new { id = item.DoctorId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.DoctorId })
            </td>
        </tr>
    }

</table>
